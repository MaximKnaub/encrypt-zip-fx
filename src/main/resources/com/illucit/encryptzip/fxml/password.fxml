<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" minHeight="180.0" prefHeight="230.0" prefWidth="480.0" styleClass="airpad" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.illucit.encryptzip.password.PasswordController">
    <children>
        <Label fx:id="hint" contentDisplay="CENTER" layoutX="46.0" layoutY="29.0" prefHeight="28.0" prefWidth="504.0" text="Please enter a password to encrypt your ZIP files with." textAlignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
        <TextField fx:id="password" layoutX="196.0" layoutY="71.0" onKeyReleased="#checkPasswords" prefHeight="29.0" prefWidth="370.0" AnchorPane.rightAnchor="0.0" />
        <TextField fx:id="confirmPassword" layoutX="185.0" layoutY="126.0" onKeyReleased="#checkPasswords" prefHeight="29.0" prefWidth="370.0" AnchorPane.rightAnchor="0.0" />
        <Label layoutX="25.0" layoutY="77.0" prefHeight="17.0" prefWidth="73.0" text="Password" AnchorPane.leftAnchor="0.0" />
        <Label layoutX="25.0" layoutY="132.0" text="Confirm Password" AnchorPane.leftAnchor="0.0" />
        <Button fx:id="ok" layoutX="487.0" layoutY="182.0" mnemonicParsing="false" onAction="#ok" text="Continue" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
        <Button layoutX="159.0" layoutY="180.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="65.0" text="Exit" />
    </children>
   <padding>
      <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
   </padding>
</AnchorPane>
